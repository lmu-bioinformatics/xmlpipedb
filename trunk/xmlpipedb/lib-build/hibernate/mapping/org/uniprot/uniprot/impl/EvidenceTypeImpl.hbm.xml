<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
        auto-import="false"
>
    <class
        name="org.uniprot.uniprot.impl.EvidenceTypeImpl"
        table="EvidenceType"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
    >

        <id
            name="idInternal"
            column="idInternal"
            type="java.lang.String"
            length="32"
            unsaved-value="null"
        >
            <generator class="uuid.hex">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-EvidenceTypeImpl.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="type"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="type"
        />

        <property
            name="key"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="key"
        />

        <property
            name="attribute"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="attribute"
        />

        <property
            name="date"
            type="de.fzi.dbs.jaxb.addon.hibernate.UTCCalendarType"
            update="true"
            insert="true"
            access="property"
            column="date"
        />

        <property
            name="category"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="category"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-EvidenceTypeImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
