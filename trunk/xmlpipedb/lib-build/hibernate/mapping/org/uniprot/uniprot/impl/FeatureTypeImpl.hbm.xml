<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
        auto-import="false"
>
    <class
        name="org.uniprot.uniprot.impl.FeatureTypeImpl"
        table="FeatureType"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
    >

        <id
            name="idInternal"
            column="idInternal"
            type="java.lang.String"
            length="32"
            unsaved-value="null"
        >
            <generator class="uuid.hex">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-FeatureTypeImpl.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="original"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="original"
        />

        <property
            name="type"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="type"
        />

        <property
            name="status"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="status"
        />

        <property
            name="ref"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ref"
        />

        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="description"
        />

        <property
            name="evidence"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="evidence"
        />

        <many-to-one
            name="location"
            class="org.uniprot.uniprot.impl.LocationTypeImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="location"
        />

        <property
            name="id"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="id"
        />

        <list
            name="variationInternal"
            table="FeatureType_VariationInternal"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            where="VariationInternal_index is not null"
        >

              <key
                  column="FeatureTypeImpl_id"
              >
              </key>

              <index
                  column="VariationInternal_index"
              />

              <element
                  column="value"
                  type="java.lang.String"
                  not-null="false"
                  unique="false"
              />

        </list>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-FeatureTypeImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
