<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
        auto-import="false"
>
    <class
        name="org.uniprot.uniprot.impl.UniprotTypeImpl"
        table="UniprotType"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
    >

        <id
            name="idInternal"
            column="idInternal"
            type="java.lang.String"
            length="32"
            unsaved-value="null"
        >
            <generator class="uuid.hex">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-UniprotTypeImpl.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="copyright"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="copyright"
        />

        <list
            name="entryInternal"
            table="UniprotType_EntryInternal"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            where="EntryInternal_index is not null"
        >

              <key
                  column="UniprotTypeImpl_id"
              >
              </key>

              <index
                  column="EntryInternal_index"
              />

              <one-to-many
                  class="org.uniprot.uniprot.impl.UniprotTypeImpl$EntryImpl"
              />

        </list>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-UniprotTypeImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

        <joined-subclass
            name="org.uniprot.uniprot.impl.UniprotImpl"
            table="Uniprot"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="parentid"
        />

        </joined-subclass>

    </class>

</hibernate-mapping>
