<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
        auto-import="false"
>
    <class
        name="org.uniprot.uniprot.impl.CitationTypeImpl"
        table="CitationType"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
    >

        <id
            name="idInternal"
            column="idInternal"
            type="java.lang.String"
            length="32"
            unsaved-value="null"
        >
            <generator class="uuid.hex">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-CitationTypeImpl.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="db"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="db"
        />

        <property
            name="first"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="first"
        />

        <property
            name="type"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="type"
        />

        <property
            name="institute"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="institute"
        />

        <property
            name="publisher"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="publisher"
        />

        <property
            name="locator"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="locator"
        />

        <property
            name="date"
            type="java.io.Serializable"
            update="true"
            insert="true"
            access="property"
            column="date"
        />

        <property
            name="city"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="city"
        />

        <property
            name="last"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="last"
        />

        <many-to-one
            name="authorList"
            class="org.uniprot.uniprot.impl.NameListTypeImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="authorList"
        />

        <property
            name="number"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="number"
        />

        <property
            name="country"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="country"
        />

        <property
            name="volume"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="volume"
        />

        <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="title"
        />

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
        />

        <many-to-one
            name="editorList"
            class="org.uniprot.uniprot.impl.NameListTypeImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="editorList"
        />

        <many-to-one
            name="citingCitation"
            class="org.uniprot.uniprot.impl.CitationTypeImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="citingCitation"
        />

        <list
            name="dbReferenceInternal"
            table="CitationType_DbReferenceInternal"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            where="DbReferenceInternal_index is not null"
        >

              <key
                  column="CitationTypeImpl_id"
              >
              </key>

              <index
                  column="DbReferenceInternal_index"
              />

              <one-to-many
                  class="org.uniprot.uniprot.impl.DbReferenceTypeImpl"
              />

        </list>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-CitationTypeImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
