<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
        auto-import="false"
>
    <class
        name="org.uniprot.uniprot.impl.OrganismTypeImpl"
        table="OrganismType"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
    >

        <id
            name="idInternal"
            column="idInternal"
            type="java.lang.String"
            length="32"
            unsaved-value="null"
        >
            <generator class="uuid.hex">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-OrganismTypeImpl.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="key"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="key"
        />

        <many-to-one
            name="lineage"
            class="org.uniprot.uniprot.impl.OrganismTypeImpl$LineageTypeImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="lineage"
        />

        <list
            name="dbReferenceInternal"
            table="OrganismType_DbReferenceInternal"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            where="DbReferenceInternal_index is not null"
        >

              <key
                  column="OrganismTypeImpl_id"
              >
              </key>

              <index
                  column="DbReferenceInternal_index"
              />

              <one-to-many
                  class="org.uniprot.uniprot.impl.DbReferenceTypeImpl"
              />

        </list>

        <list
            name="nameInternal"
            table="OrganismType_NameInternal"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            where="NameInternal_index is not null"
        >

              <key
                  column="OrganismTypeImpl_id"
              >
              </key>

              <index
                  column="NameInternal_index"
              />

              <one-to-many
                  class="org.uniprot.uniprot.impl.OrganismNameTypeImpl"
              />

        </list>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-OrganismTypeImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
