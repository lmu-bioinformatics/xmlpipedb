<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
        auto-import="false"
>
    <class
        name="org.uniprot.uniprot.impl.ProteinTypeImpl"
        table="ProteinType"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
    >

        <id
            name="idInternal"
            column="idInternal"
            type="java.lang.String"
            length="32"
            unsaved-value="null"
        >
            <generator class="uuid.hex">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-ProteinTypeImpl.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="type"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="type"
        />

        <property
            name="evidence"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="evidence"
        />

        <list
            name="domainInternal"
            table="ProteinType_DomainInternal"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            where="DomainInternal_index is not null"
        >

              <key
                  column="ProteinTypeImpl_id"
              >
              </key>

              <index
                  column="DomainInternal_index"
              />

              <one-to-many
                  class="org.uniprot.uniprot.impl.ProteinTypeImpl$DomainTypeImpl"
              />

        </list>

        <list
            name="componentInternal"
            table="ProteinType_ComponentInternal"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            where="ComponentInternal_index is not null"
        >

              <key
                  column="ProteinTypeImpl_id"
              >
              </key>

              <index
                  column="ComponentInternal_index"
              />

              <one-to-many
                  class="org.uniprot.uniprot.impl.ProteinTypeImpl$ComponentTypeImpl"
              />

        </list>

        <list
            name="nameInternal"
            table="ProteinType_NameInternal"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            where="NameInternal_index is not null"
        >

              <key
                  column="ProteinTypeImpl_id"
              >
              </key>

              <index
                  column="NameInternal_index"
              />

              <one-to-many
                  class="org.uniprot.uniprot.impl.ProteinNameTypeImpl"
              />

        </list>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ProteinTypeImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
