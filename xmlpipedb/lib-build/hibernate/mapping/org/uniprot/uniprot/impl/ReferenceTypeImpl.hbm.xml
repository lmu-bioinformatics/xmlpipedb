<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
        auto-import="false"
>
    <class
        name="org.uniprot.uniprot.impl.ReferenceTypeImpl"
        table="ReferenceType"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
    >

        <id
            name="idInternal"
            column="idInternal"
            type="java.lang.String"
            length="32"
            unsaved-value="null"
        >
            <generator class="uuid.hex">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-ReferenceTypeImpl.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="key"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="key"
        />

        <many-to-one
            name="citation"
            class="org.uniprot.uniprot.impl.CitationTypeImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="citation"
        />

        <property
            name="evidence"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="evidence"
        />

        <many-to-one
            name="source"
            class="org.uniprot.uniprot.impl.SourceDataTypeImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="source"
        />

        <list
            name="scopeInternal"
            table="ReferenceType_ScopeInternal"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            where="ScopeInternal_index is not null"
        >

              <key
                  column="ReferenceTypeImpl_id"
              >
              </key>

              <index
                  column="ScopeInternal_index"
              />

              <element
                  column="value"
                  type="java.lang.String"
                  not-null="false"
                  unique="false"
              />

        </list>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ReferenceTypeImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
