<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
        auto-import="false"
>
    <class
        name="org.uniprot.uniprot.impl.SourceDataTypeImpl$SpeciesOrStrainOrPlasmidImpl"
        table="SourceDataType$SpeciesOrStrainOrPlasmid"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
    >

        <id
            name="idInternal"
            column="idInternal"
            type="java.lang.String"
            length="32"
            unsaved-value="null"
        >
            <generator class="uuid.hex">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SourceDataTypeImpl.SpeciesOrStrainOrPlasmidImpl.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="plasmid"
            class="org.uniprot.uniprot.impl.SourceDataTypeImpl$PlasmidImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="plasmid"
        />

        <many-to-one
            name="tissue"
            class="org.uniprot.uniprot.impl.SourceDataTypeImpl$TissueImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="tissue"
        />

        <many-to-one
            name="strain"
            class="org.uniprot.uniprot.impl.SourceDataTypeImpl$StrainImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="strain"
        />

        <many-to-one
            name="transposon"
            class="org.uniprot.uniprot.impl.SourceDataTypeImpl$TransposonImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="transposon"
        />

        <many-to-one
            name="species"
            class="org.uniprot.uniprot.impl.SourceDataTypeImpl$SpeciesImpl"
            cascade="all"
            outer-join="auto"
            update="true"
            insert="true"
            access="property"
            column="species"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SourceDataTypeImpl.SpeciesOrStrainOrPlasmidImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
