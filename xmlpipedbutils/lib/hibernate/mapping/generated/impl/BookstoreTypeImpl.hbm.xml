<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
        auto-import="false"
>
    <class
        name="generated.impl.BookstoreTypeImpl"
        table="BookstoreType"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
    >

        <id
            name="idInternal"
            column="idInternal"
            type="java.lang.String"
            length="32"
            unsaved-value="null"
        >
            <generator class="uuid.hex">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-BookstoreTypeImpl.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <list
            name="bookInternal"
            table="BookstoreType_BookInternal"
            lazy="false"
            inverse="false"
            cascade="all-delete-orphan"
            where="BookInternal_index is not null"
        >

              <key
                  column="BookstoreTypeImpl_id"
              >
              </key>

              <index
                  column="BookInternal_index"
              />

              <one-to-many
                  class="generated.impl.BookstoreTypeImpl$BookImpl"
              />

        </list>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-BookstoreTypeImpl.xml
            containing the additional properties and place it in your merge dir.
        -->

        <joined-subclass
            name="generated.impl.BookstoreImpl"
            table="Bookstore"
            dynamic-update="false"
            dynamic-insert="false"
        >
        <key
            column="parentid"
        />

        </joined-subclass>

    </class>

</hibernate-mapping>
